{
	"name": "@dluznicek/client",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"author": "Jan Kom√≠nek",
	"keywords": [
		"dluznicek",
		"client"
	],
	"scripts": {
		"upd": "pnpm update && pnpm upgrade && pnpm install",
		"upd:f": "pnpm update --force && pnpm upgrade --force && pnpm install --force",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write .",
		"precommit": "prettier --plugin-search-dir . --write . && prettier --plugin-search-dir . --check . && eslint . --fix && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json"
	},
	"dependencies": {
		"@sveltejs/adapter-cloudflare": "^2.3.3",
		"axios": "^1.5.1",
		"classnames": "^2.3.2",
		"jose": "^4.15.3"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^2.1.0",
		"@sveltejs/kit": "^1.25.2",
		"@types/events": "^3.0.1",
		"@types/node": "^20.8.5",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.51.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"postcss": "^8.4.31",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"prettier-plugin-tailwindcss": "^0.2.8",
		"svelte": "^3.59.2",
		"svelte-check": "^3.5.2",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.6.2",
		"typescript": "^4.9.5",
		"vite": "^4.4.11"
	}
}
